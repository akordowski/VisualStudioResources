<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>nua_Author</Title>
      <Shortcut>nua_Author</Shortcut>
      <Description>Code snippet for an NUnit Author Attribute</Description>
      <Author>Artur Kordowski</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>AuthorName</ID>
          <ToolTip>Replace with the name of the author</ToolTip>
          <Default>Author</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[[Author("$AuthorName$")]$end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>