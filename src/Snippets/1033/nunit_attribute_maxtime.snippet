<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>nua_MaxTime</Title>
      <Shortcut>nua_MaxTime</Shortcut>
      <Description>Code snippet for an NUnit MaxTime Attribute</Description>
      <Author>Artur Kordowski</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>MaxTime</ID>
          <ToolTip>Replace with the max time</ToolTip>
          <Default>2000</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[[MaxTime($MaxTime$)]$end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>