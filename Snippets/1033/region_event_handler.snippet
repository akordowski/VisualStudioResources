<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>#region_event_handler</Title>
      <Shortcut>#region_event_handler</Shortcut>
      <Description>Code snippet for #region Event Handler</Description>
      <Author>Artur Kordowski</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>AccessModifier</ID>
          <ToolTip>Replace with the access modifier</ToolTip>
          <Default>Private</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[#region $AccessModifier$ Event Handler
      $selected$ $end$
      #endregion]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>