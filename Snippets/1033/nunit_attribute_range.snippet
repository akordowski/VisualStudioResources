<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>nua_Range</Title>
      <Shortcut>nua_Range</Shortcut>
      <Description>Code snippet for an NUnit Range Attribute</Description>
      <Author>Artur Kordowski</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>RangeFrom</ID>
          <ToolTip>Replace with the from value</ToolTip>
          <Default>1</Default>
        </Literal>
      </Declarations>
      <Declarations>
        <Literal>
          <ID>RangeTo</ID>
          <ToolTip>Replace with the to value</ToolTip>
          <Default>10</Default>
        </Literal>
      </Declarations>
      <Declarations>
        <Literal>
          <ID>RangeStep</ID>
          <ToolTip>Replace with the step value</ToolTip>
          <Default>1</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[[Range($RangeFrom$, $RangeTo$, $RangeStep$)]$end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>